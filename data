   "source": [
    "# 형태소 분석\n",
    "food_list = df['곁들일 음식'].values.tolist()\n",
    "text = ''.join(food_list) # list들을 공백으로 구분해서 연결\n",
    "\n",
    "# konlpy \n",
    "from konlpy.tag import Okt\n",
    "from collections import Counter\n",
    "okt = Okt()\n",
    "\n",
    "morph = okt.pos(text)\n",
    "morph\n",
    "\n",
    "noun_list = []\n",
    "for word, tag in morph:\n",
    "    if(tag=='Noun')&(len(word)>1):\n",
    "        noun_list.append(word)\n",
    "count = Counter(noun_list)\n",
    "\n",
    "data_df = dict(count.most_common(350))\n",
    "series = pd.Series(data_df)\n",
    "df_df = pd.DataFrame(series, columns= ['cnt'])\n",
    "df_df[:50]"
   ]
  },
