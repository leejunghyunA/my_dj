  {
   "cell_type": "code",
   "execution_count": 259,
   "id": "6975ce1c",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 음식에 있는 단어 나타나면 key값으로 대체\n",
    "def food_match(df, dic_list):\n",
    "    new_foods = []\n",
    "    for index in range(len(df)):\n",
    "        foods = df.loc[index, '곁들일 음식']\n",
    "        for i in range(len(dic_list)):\n",
    "            # key랑 value 나누기\n",
    "            values = dic_list[i][1]\n",
    "            keys = dic_list[i][0]\n",
    "            for text in values:\n",
    "                if text in foods:\n",
    "                    new_foods.append(keys)\n",
    "        df.loc[index, '음식'] = list(set(new_foods))\n",
    "    return df[['곁들일 음식', '음식']]"
   ]
  },
